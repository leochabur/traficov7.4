<?php



use Doctrine\ORM\Mapping as ORM;

/**
 * MotivoTAGMaestro
 */
class MotivoTAGMaestro
{
    /**
     * @var string
     */
    private $motivo;

    /**
     * @var integer
     */
    private $id;


    /**
     * Set motivo
     *
     * @param string $motivo
     * @return MotivoTAGMaestro
     */
    public function setMotivo($motivo)
    {
        $this->motivo = $motivo;

        return $this;
    }

    /**
     * Get motivo
     *
     * @return string 
     */
    public function getMotivo()
    {
        return $this->motivo;
    }

    /**
     * Set id
     *
     * @param integer $id
     * @return MotivoTAGMaestro
     */
    public function setId($id)
    {
        $this->id = $id;

        return $this;
    }

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    public function __toString()
    {
        return $this->motivo;
    }
}
