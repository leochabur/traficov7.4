<?php



use Doctrine\ORM\Mapping as ORM;

/**
 * Servicio
 */
class Servicio
{
    /**
     * @var \DateTime
     */
    private $citacion;

    /**
     * @var \DateTime
     */
    private $salida;

    /**
     * @var \DateTime
     */
    private $llegada;

    /**
     * @var \DateTime
     */
    private $finServicio;

    /**
     * @var string
     */
    private $idaVelta;

    /**
     * @var boolean
     */
    private $activo;

    /**
     * @var integer
     */
    private $id;

    /**
     * @var \Estructura
     */
    private $estructura;

    /**
     * @var \Cronograma
     */
    private $cronograma;

    /**
     * @var \TurnoServicio
     */
    private $turno;

    /**
     * @var \TipoServicio
     */
    private $tipo;


    /**
     * Set citacion
     *
     * @param \DateTime $citacion
     * @return Servicio
     */
    public function setCitacion($citacion)
    {
        $this->citacion = $citacion;

        return $this;
    }

    /**
     * Get citacion
     *
     * @return \DateTime 
     */
    public function getCitacion()
    {
        return $this->citacion;
    }

    /**
     * Set salida
     *
     * @param \DateTime $salida
     * @return Servicio
     */
    public function setSalida($salida)
    {
        $this->salida = $salida;

        return $this;
    }

    /**
     * Get salida
     *
     * @return \DateTime 
     */
    public function getSalida()
    {
        return $this->salida;
    }

    /**
     * Set llegada
     *
     * @param \DateTime $llegada
     * @return Servicio
     */
    public function setLlegada($llegada)
    {
        $this->llegada = $llegada;

        return $this;
    }

    /**
     * Get llegada
     *
     * @return \DateTime 
     */
    public function getLlegada()
    {
        return $this->llegada;
    }

    /**
     * Set finServicio
     *
     * @param \DateTime $finServicio
     * @return Servicio
     */
    public function setFinServicio($finServicio)
    {
        $this->finServicio = $finServicio;

        return $this;
    }

    /**
     * Get finServicio
     *
     * @return \DateTime 
     */
    public function getFinServicio()
    {
        return $this->finServicio;
    }

    /**
     * Set idaVelta
     *
     * @param string $idaVelta
     * @return Servicio
     */
    public function setIdaVelta($idaVelta)
    {
        $this->idaVelta = $idaVelta;

        return $this;
    }

    /**
     * Get idaVelta
     *
     * @return string 
     */
    public function getIdaVelta()
    {
        return $this->idaVelta;
    }

    /**
     * Set activo
     *
     * @param boolean $activo
     * @return Servicio
     */
    public function setActivo($activo)
    {
        $this->activo = $activo;

        return $this;
    }

    /**
     * Get activo
     *
     * @return boolean 
     */
    public function getActivo()
    {
        return $this->activo;
    }

    /**
     * Set id
     *
     * @param integer $id
     * @return Servicio
     */
    public function setId($id)
    {
        $this->id = $id;

        return $this;
    }

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set estructura
     *
     * @param \Estructura $estructura
     * @return Servicio
     */
    public function setEstructura(\Estructura $estructura)
    {
        $this->estructura = $estructura;

        return $this;
    }

    /**
     * Get estructura
     *
     * @return \Estructura 
     */
    public function getEstructura()
    {
        return $this->estructura;
    }

    /**
     * Set cronograma
     *
     * @param \Cronograma $cronograma
     * @return Servicio
     */
    public function setCronograma(\Cronograma $cronograma)
    {
        $this->cronograma = $cronograma;

        return $this;
    }

    /**
     * Get cronograma
     *
     * @return \Cronograma 
     */
    public function getCronograma()
    {
        return $this->cronograma;
    }

    /**
     * Set turno
     *
     * @param \TurnoServicio $turno
     * @return Servicio
     */
    public function setTurno(\TurnoServicio $turno)
    {
        $this->turno = $turno;

        return $this;
    }

    /**
     * Get turno
     *
     * @return \TurnoServicio 
     */
    public function getTurno()
    {
        return $this->turno;
    }

    /**
     * Set tipo
     *
     * @param \TipoServicio $tipo
     * @return Servicio
     */
    public function setTipo(\TipoServicio $tipo)
    {
        $this->tipo = $tipo;

        return $this;
    }

    /**
     * Get tipo
     *
     * @return \TipoServicio 
     */
    public function getTipo()
    {
        return $this->tipo;
    }
}
