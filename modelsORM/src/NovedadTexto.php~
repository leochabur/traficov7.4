<?php



use Doctrine\ORM\Mapping as ORM;

/**
 * NovedadTexto
 */
class NovedadTexto
{
    /**
     * @var string
     */
    private $texto;

    /**
     * @var string
     */
    private $disponibilidad;

    /**
     * @var string
     */
    private $variable;

    /**
     * @var float
     */
    private $puntaje;

    /**
     * @var string
     */
    private $descripcion;

    /**
     * @var boolean
     */
    private $activa;

    /**
     * @var boolean
     */
    private $afecta_diagrama;

    /**
     * @var boolean
     */
    private $codOrigen;

    /**
     * @var boolean
     */
    private $codDestino;

    /**
     * @var boolean
     */
    private $afecta_incentivo;

    /**
     * @var integer
     */
    private $id;

    /**
     * @var \Estructura
     */
    private $estructura;


    /**
     * Set texto
     *
     * @param string $texto
     * @return NovedadTexto
     */
    public function setTexto($texto)
    {
        $this->texto = $texto;

        return $this;
    }

    /**
     * Get texto
     *
     * @return string 
     */
    public function getTexto()
    {
        return $this->texto;
    }

    /**
     * Set disponibilidad
     *
     * @param string $disponibilidad
     * @return NovedadTexto
     */
    public function setDisponibilidad($disponibilidad)
    {
        $this->disponibilidad = $disponibilidad;

        return $this;
    }

    /**
     * Get disponibilidad
     *
     * @return string 
     */
    public function getDisponibilidad()
    {
        return $this->disponibilidad;
    }

    /**
     * Set variable
     *
     * @param string $variable
     * @return NovedadTexto
     */
    public function setVariable($variable)
    {
        $this->variable = $variable;

        return $this;
    }

    /**
     * Get variable
     *
     * @return string 
     */
    public function getVariable()
    {
        return $this->variable;
    }

    /**
     * Set puntaje
     *
     * @param float $puntaje
     * @return NovedadTexto
     */
    public function setPuntaje($puntaje)
    {
        $this->puntaje = $puntaje;

        return $this;
    }

    /**
     * Get puntaje
     *
     * @return float 
     */
    public function getPuntaje()
    {
        return $this->puntaje;
    }

    /**
     * Set descripcion
     *
     * @param string $descripcion
     * @return NovedadTexto
     */
    public function setDescripcion($descripcion)
    {
        $this->descripcion = $descripcion;

        return $this;
    }

    /**
     * Get descripcion
     *
     * @return string 
     */
    public function getDescripcion()
    {
        return $this->descripcion;
    }

    /**
     * Set activa
     *
     * @param boolean $activa
     * @return NovedadTexto
     */
    public function setActiva($activa)
    {
        $this->activa = $activa;

        return $this;
    }

    /**
     * Get activa
     *
     * @return boolean 
     */
    public function getActiva()
    {
        return $this->activa;
    }

    /**
     * Set afecta_diagrama
     *
     * @param boolean $afectaDiagrama
     * @return NovedadTexto
     */
    public function setAfectaDiagrama($afectaDiagrama)
    {
        $this->afecta_diagrama = $afectaDiagrama;

        return $this;
    }

    /**
     * Get afecta_diagrama
     *
     * @return boolean 
     */
    public function getAfectaDiagrama()
    {
        return $this->afecta_diagrama;
    }

    /**
     * Set codOrigen
     *
     * @param boolean $codOrigen
     * @return NovedadTexto
     */
    public function setCodOrigen($codOrigen)
    {
        $this->codOrigen = $codOrigen;

        return $this;
    }

    /**
     * Get codOrigen
     *
     * @return boolean 
     */
    public function getCodOrigen()
    {
        return $this->codOrigen;
    }

    /**
     * Set codDestino
     *
     * @param boolean $codDestino
     * @return NovedadTexto
     */
    public function setCodDestino($codDestino)
    {
        $this->codDestino = $codDestino;

        return $this;
    }

    /**
     * Get codDestino
     *
     * @return boolean 
     */
    public function getCodDestino()
    {
        return $this->codDestino;
    }

    /**
     * Set afecta_incentivo
     *
     * @param boolean $afectaIncentivo
     * @return NovedadTexto
     */
    public function setAfectaIncentivo($afectaIncentivo)
    {
        $this->afecta_incentivo = $afectaIncentivo;

        return $this;
    }

    /**
     * Get afecta_incentivo
     *
     * @return boolean 
     */
    public function getAfectaIncentivo()
    {
        return $this->afecta_incentivo;
    }

    /**
     * Set id
     *
     * @param integer $id
     * @return NovedadTexto
     */
    public function setId($id)
    {
        $this->id = $id;

        return $this;
    }

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set estructura
     *
     * @param \Estructura $estructura
     * @return NovedadTexto
     */
    public function setEstructura(\Estructura $estructura)
    {
        $this->estructura = $estructura;

        return $this;
    }

    /**
     * Get estructura
     *
     * @return \Estructura 
     */
    public function getEstructura()
    {
        return $this->estructura;
    }
    /**
     * @var boolean
     */
    private $isFeriado;

    /**
     * @var boolean
     */
    private $isFranco;


    /**
     * Set isFeriado
     *
     * @param boolean $isFeriado
     * @return NovedadTexto
     */
    public function setIsFeriado($isFeriado)
    {
        $this->isFeriado = $isFeriado;

        return $this;
    }

    /**
     * Get isFeriado
     *
     * @return boolean 
     */
    public function getIsFeriado()
    {
        return $this->isFeriado;
    }

    /**
     * Set isFranco
     *
     * @param boolean $isFranco
     * @return NovedadTexto
     */
    public function setIsFranco($isFranco)
    {
        $this->isFranco = $isFranco;

        return $this;
    }

    /**
     * Get isFranco
     *
     * @return boolean 
     */
    public function getIsFranco()
    {
        return $this->isFranco;
    }
    /**
     * @var boolean
     */
    private $compensatorio; 


    /**
     * Set compensatorio
     *
     * @param boolean $compensatorio
     * @return NovedadTexto
     */
    public function setCompensatorio($compensatorio)
    {
        $this->compensatorio = $compensatorio;

        return $this;
    }

    /**
     * Get compensatorio
     *
     * @return boolean 
     */
    public function getCompensatorio()
    {
        return $this->compensatorio;
    }
}
