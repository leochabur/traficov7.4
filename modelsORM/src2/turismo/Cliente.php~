<?php


/**
 * @author Leo
 * @version 1.0
 * @created 07-mar.-2018 9:05:15 p. m.
 */
class Cliente
{

	var $id;
	var $razonSocial;
	var $cuit;
	var $activo;
	var $estructura;

	function Cliente()
	{
	}



	function getId()
	{
		return $this->id;
	}

	function getRazonSocial()
	{
		return $this->razonSocial;
	}

	function getActivo()
	{
		return $this->activo;
	}
	function getEstructura()
	{
		return $this->estructura;
	}	

	/**
	 * 
	 * @param newVal
	 */
	function setRazonSocial($newVal)
	{
		$this->razonSocial = $newVal;
	}

	function getCuit()
	{
		return $this->cuit;
	}

	/**
	 * 
	 * @param newVal
	 */
	function setCuit($newVal)
	{
		$this->cuit = $newVal;
	}

	function setActivo($newVal)
	{
		$this->activo = $newVal;
	}

	function setEstructura($newVal)
	{
		$this->estructura = $newVal;
	}

	public function __toString()
	{
		return strtoupper($this->razonSocial);
	}	

}
